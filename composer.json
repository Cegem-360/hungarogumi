{
    "$schema": "https://getcomposer.org/schema.json",
    "name": "dcrepper/filament-starter-kit",
    "type": "project",
    "description": "The skeleton application for the Laravel framework.",
    "keywords": [
        "laravel",
        "framework"
    ],
    "license": "MIT",
    "require": {
        "php": "^8.3",
        "filament/filament": "^5.0",
        "guzzlehttp/guzzle": "^7.9",
        "laravel/framework": "^12.0",
        "laravel/tinker": "^2.10.1",
        "livewire/livewire": "^4.0",
        "nunomaduro/essentials": "^0.1",
        "spatie/laravel-permission": "^6.20"
    },
    "require-dev": {
        "agencetwogether/hookshelper": "*",
        "barryvdh/laravel-debugbar": "*",
        "barryvdh/laravel-ide-helper": "*",
        "driftingly/rector-laravel": "*",
        "fakerphp/faker": "*",
        "filament/blueprint": "*",
        "filament/upgrade": "^5.0",
        "laravel/boost": "*",
        "laravel/pail": "*",
        "laravel/pint": "*",
        "laravel/sail": "*",
        "mockery/mockery": "*",
        "nunomaduro/collision": "*",
        "pestphp/pest": "*",
        "pestphp/pest-plugin-faker": "*",
        "pestphp/pest-plugin-laravel": "*",
        "pestphp/pest-plugin-livewire": "*",
        "phpseclib/phpseclib": "*",
        "rector/rector": "*"
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/",
            "Database\\Factories\\": "database/factories/",
            "Database\\Seeders\\": "database/seeders/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "scripts": {
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover --ansi",
            "@php artisan filament:upgrade"
        ],
        "post-update-cmd": [
            "@php artisan vendor:publish --tag=laravel-assets --ansi --force"
        ],
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate --ansi",
            "@php -r \"file_exists('database/database.sqlite') || touch('database/database.sqlite');\"",
            "@php artisan migrate --graceful --ansi"
        ],
        "dev": [
            "Composer\\Config::disableProcessTimeout",
            "npx concurrently -c \"#93c5fd,#c4b5fd,#fb7185,#fdba74\" \"php artisan serve\" \"php artisan queue:listen --tries=1\" \"php artisan pail --timeout=0\" \"npm run dev\" --names=server,queue,logs,vite"
        ],
        "test": [
            "@php artisan config:clear --ansi",
            "@php artisan test"
        ],
        "rector": [
            "@php vendor/bin/rector process --ansi"
        ],
        "pint": [
            "@php vendor/bin/pint --test"
        ],
        "pint-fix": [
            "@php vendor/bin/pint"
        ]
    },
    "extra": {
        "laravel": {
            "dont-discover": []
        }
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true,
        "allow-plugins": {
            "pestphp/pest-plugin": true,
            "php-http/discovery": true
        }
    },
    "minimum-stability": "beta",
    "prefer-stable": true,
    "repositories": [
        {
            "name": "filament",
            "type": "composer",
            "url": "https://packages.filamentphp.com/composer"
        }
    ]
}